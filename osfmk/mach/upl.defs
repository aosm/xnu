/*
 * File:	mach/upl.defs
 *
 * Abstract:
 *	Basic Mach external memory management interface declaration.
 */

subsystem
#if	KERNEL_USER
	  KernelUser
#endif	KERNEL_USER
#if	KERNEL_SERVER
	  KernelServer
#endif	KERNEL_SERVER
		     upl 2050;

#ifdef	MACH_KERNEL
#include <advisory_pageout.h>
#endif	/* MACH_KERNEL */

#include <mach/std_types.defs>
#include <mach/mach_types.defs>

routine upl_abort(
		upl_object	: upl_t;
	in	abort_cond	: integer_t);

routine upl_abort_range(
		upl_object	: upl_t;
		offset		: vm_offset_t;
		size		: vm_size_t;
	in	abort_cond	: integer_t;
	out	empty	: boolean_t);
 
routine upl_commit(
		upl_object	: upl_t;
	in	page_list	: upl_page_info_array_t);

routine upl_commit_range(
		upl_object	: upl_t;
		offset		: vm_offset_t;
		size		: vm_size_t;
	in	cntrl_flags	: integer_t;
	in	page_list	: upl_page_info_array_t;
	out	empty	: boolean_t);

